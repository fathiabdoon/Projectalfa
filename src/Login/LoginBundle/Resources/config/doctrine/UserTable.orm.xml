<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Login\LoginBundle\Entity\UserTable" table="user_table">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userName" type="string" column="user_name" length="100" nullable="false"/>
    <field name="userFirstname" type="string" column="user_firstname" length="100" nullable="false"/>
    <field name="userEmail" type="string" column="user_email" length="200" nullable="false"/>
    <field name="userPassword" type="string" column="user_password" length="200" nullable="false"/>
  </entity>
</doctrine-mapping>
